{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Frame-Options", "value": "SAMEORIGIN" },
        { "key": "Content-Security-Policy", "value": "frame-ancestors 'self' https://mann.cool https://www.mann.cool https://*.mann.cool http://localhost:*" }
      ]
    }
  ]
}
